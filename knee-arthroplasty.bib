@article{Straat2023,
   author = {Straat, A. C. and Maarleveld, J. M. and Smit, D. J. M. and Visch, L. and Hulsegge, G. and Huirne, J. A. F. and van Dongen, J. M. and van Geenen, R. C. and Kerkhoffs, Gmmj and Anema, J. R. and Coenen, P. and Kuijer, Ppfm},
   title = {(Cost-)effectiveness of a personalized multidisciplinary eHealth intervention for knee arthroplasty patients to enhance return to activities of daily life, work and sports - rationale and protocol of the multicentre ACTIVE randomized controlled trial},
   journal = {BMC Musculoskelet Disord},
